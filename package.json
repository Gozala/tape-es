{
  "name": "tape-es",
  "version": "1.1.2",
  "description": "ESM-compatible Tape.js test runner and test watcher",
  "keywords": [
    "testing",
    "tape",
    "tap",
    "test",
    "watch",
    "harness",
    "esm"
  ],
  "repository": "https://github.com/vanillaes/tape-es.git",
  "author": "Evan Plaice <evanplaice@gmail.com>",
  "license": "MIT",
  "bin": {
    "tape-es": "bin/tape-es",
    "tape-watch-es": "bin/tape-watch-es"
  },
  "type": "module",
  "engines": {
    "node" : ">13.2"
  },
  "scripts": {
    "lint": "semistandard",
    "package": "npx rimraf package && npm pack | tail -n 1 | xargs tar -xf",
    "preversion": "npm run lint",
    "postversion": "git push --follow-tags"
  },
  "dependencies": {
    "chokidar": "^3.3.0",
    "commander": "^4.0.1",
    "glob": "^7.1.6",
    "tape": "^4.11.0"
  },
  "devDependencies": {
    "semistandard": "^14.2.0"
  }
}
