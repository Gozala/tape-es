{
  "name": "tape-es",
  "version": "1.0.2",
  "description": "ESM-compatible Tape.js runner",
  "keywords": [
    "testing",
    "tape",
    "tap",
    "test",
    "harness",
    "esm"
  ],
  "repository": "https://github.com/vanillaes/tape-es.git",
  "author": "Evan Plaice <evanplaice@gmail.com>",
  "license": "MIT",
  "bin": {
    "tape-es": "bin/tape-es"
  },
  "type": "module",
  "scripts": {
    "lint": "semistandard",
    "package": "npx rimraf package && npm pack | tail -n 1 | xargs tar -xf",
    "preversion": "npm run lint",
    "postversion": "git push --follow-tags"
  },
  "dependencies": {
    "commander": "^4.0.1",
    "glob": "^7.1.6",
    "tape": "^4.11.0"
  },
  "devDependencies": {
    "semistandard": "^14.2.0"
  }
}
